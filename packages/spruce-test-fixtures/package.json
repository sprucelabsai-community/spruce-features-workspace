{
  "name": "@sprucelabs/spruce-test-fixtures",
  "publishConfig": {
    "access": "public"
  },
  "skill": {
    "namespace": "spruceTestFixtures",
    "commandOverrides": {
      "sync.schemas": "--shouldFetchCoreSchemas false"
    },
    "upgradeIgnoreList": [
      "@types/node",
      "@sprucelabs/spruce-test-fixtures"
    ]
  },
  "version": "73.0.5",
  "files": [
    "build/**/*",
    "!build/__tests__",
    "!build/esm/__tests__",
    "tsconfig.json"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "description": "Spruce Test Fixtures! ðŸ›‹",
  "keywords": [
    "node",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/spruce-features-workspace",
  "bugs": {
    "url": "https://github.com/sprucelabsai/spruce-features-workspace/issues"
  },
  "scripts": {
    "build.ci": "yarn run build.tsc && yarn run build.resolve-paths && yarn run lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn run build.tsc --sourceMap ; yarn run resolve-paths.lint",
    "build.dist": "tsc --project tsconfig.dist.json && yarn build.resolve-paths && mv build esm && yarn build.esm-postbuild && yarn build.tsc && yarn build.resolve-paths && mv esm build/ && yarn clean.dist",
    "build.esm-postbuild": "esm-postbuild --target esm --patterns '**/*.js'",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn run clean.build",
    "clean.all": "yarn run clean.dependencies && yarn run clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "clean.dist": "true",
    "fix.lint": "cd ../ && yarn fix.lint",
    "fix.lint.local": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "rebuild": "cd .. && yarn rebuild",
    "resolve-paths.lint": "yarn run build.resolve-paths ; yarn run lint",
    "test": "jest",
    "post.watch.build": "yarn run build.copy-files && yarn run build.resolve-paths",
    "spruce.upgrade": "spruce upgrade",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn run post.watch.build'",
    "watch.rebuild": "yarn run clean.all && yarn install && yarn run watch.build.dev",
    "watch.tsc": "tsc -w",
    "lint.tsc": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@sprucelabs/data-stores": "^30.0.12",
    "@sprucelabs/error": "^7.0.29",
    "@sprucelabs/heartwood-view-controllers": "^125.0.6",
    "@sprucelabs/mercury-client": "^43.0.69",
    "@sprucelabs/mercury-core-events": "^27.0.62",
    "@sprucelabs/mercury-event-emitter": "^43.0.69",
    "@sprucelabs/schema": "^32.1.3",
    "@sprucelabs/spruce-core-schemas": "^41.0.49",
    "@sprucelabs/spruce-event-utils": "^41.0.56",
    "@sprucelabs/spruce-permission-utils": "^18.0.7",
    "@sprucelabs/spruce-skill-booter": "^73.0.5",
    "@sprucelabs/spruce-skill-utils": "^32.0.52",
    "@sprucelabs/test-utils": "^6.0.45",
    "dotenv": "^17.2.1",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^7.0.19",
    "@sprucelabs/jest-json-reporter": "^9.0.31",
    "@sprucelabs/mercury-types": "^48.0.51",
    "@sprucelabs/resolve-path-aliases": "^3.0.16",
    "@sprucelabs/semantic-release": "^5.0.2",
    "@sprucelabs/test": "^10.0.13",
    "@types/node": "17.0.5",
    "calendar-utils": "^0.11.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.32.0",
    "eslint-config-spruce": "^11.2.26",
    "jest": "^30.0.5",
    "jest-circus": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "maxWorkers": 1,
    "testTimeout": 1200000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/esm/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    }
  }
}
