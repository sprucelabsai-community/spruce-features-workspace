{
  "name": "@sprucelabs/spruce-test-fixtures",
  "publishConfig": {
    "access": "public"
  },
  "skill": {
    "namespace": "spruceTestFixtures",
    "commandOverrides": {
      "sync.schemas": "--shouldFetchCoreSchemas false"
    },
    "upgradeIgnoreList": [
      "@sprucelabs/spruce-test-fixtures"
    ]
  },
  "version": "5.5.24",
  "files": [
    "build/**/*",
    "!build/__tests__",
    "!build/esm/__tests__",
    "tsconfig.json"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "description": "Spruce Test Fixtures! ðŸ›‹",
  "keywords": [
    "node",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/spruce-features-workspace",
  "bugs": {
    "url": "https://github.com/sprucelabsai/spruce-features-workspace/issues"
  },
  "scripts": {
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.dist": "yarn build.tsc && tsc --project tsconfig.dist.json && yarn build.resolve-paths && yarn build.esm-postbuild",
    "build.esm-postbuild": "esm-postbuild --target build/esm --patterns '**/*.js'",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "fix.lint": "cd ../ && yarn fix.lint",
    "fix.lint.local": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "rebuild": "cd .. && yarn rebuild",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "test": "jest",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w",
    "lint.tsc": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@sprucelabs/data-stores": "^6.1.396",
    "@sprucelabs/error": "^5.0.257",
    "@sprucelabs/heartwood-view-controllers": "^34.2.1",
    "@sprucelabs/mercury-client": "^16.13.124",
    "@sprucelabs/mercury-core-events": "^1.2.624",
    "@sprucelabs/mercury-event-emitter": "^16.13.124",
    "@sprucelabs/schema": "^25.3.377",
    "@sprucelabs/spruce-core-schemas": "^22.5.496",
    "@sprucelabs/spruce-event-utils": "^16.0.547",
    "@sprucelabs/spruce-skill-booter": "^5.5.24",
    "@sprucelabs/spruce-skill-utils": "^20.6.326",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^1.0.274",
    "@sprucelabs/jest-json-reporter": "^6.0.222",
    "@sprucelabs/jest-sheets-reporter": "^1.2.227",
    "@sprucelabs/mercury-types": "^26.1.536",
    "@sprucelabs/resolve-path-aliases": "^1.0.220",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/test": "^7.7.210",
    "@sprucelabs/test-utils": "^3.0.230",
    "@types/node": "^16.11.4",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.3.0",
    "conventional-changelog-sprucelabs": "^1.1.2",
    "eslint": "^8.1.0",
    "eslint-config-spruce": "^10.10.15",
    "jest": "^27.3.1",
    "jest-circus": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "maxWorkers": 4,
    "testTimeout": 300000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/esm/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    },
    "reporters": [
      "default",
      [
        "@sprucelabs/jest-sheets-reporter",
        {
          "sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
          "worksheetId": 843037832,
          "testMap": {
            "canGetFunctioningStores": "I90",
            "canAttachPersonToOrg": "I144"
          }
        }
      ]
    ]
  }
}
